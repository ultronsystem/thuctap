<div  id="list_book">
 <div class="w3-row-padding w3-padding-16 w3-center">
<% if @list_books.any? %>
    <% @list_books.each do |book| %>
    <div class="w3-quarter custom-height">
      <%= link_to book_path(book.id) do %>
        <%= image_tag("book.jpg")%>
      <% end %>
      <%= load_star_rating(book_star(book)) %>
      <h3><%=link_to book.title, book_path(book.id) %></h3>
    </div>
    <% end %>
<% end %>
  </div>
  <div class="w3-center w3-padding-32">
  <div class="w3-bar">
    <%= will_paginate @list_books, id: "paginate" %>
  </div>
</div>
  </div>

